
enum WorkerReport {
  Next,
  Done,
  Dead( Worker )
}

enum WorkerCommand {

}

struct Worker {
  rewrites : u32,
  stack    : WorkerStack
}

struct Scheduler {
  worker_handles : Vec<WorkerHandle>
  master_worker  : Worker,
  rewrite_quota  : u32,
  inbox          : Reciever<WorkerReport>
}

struct Runtime {
  schedulers : Scheduler,
  constants  : Arc<ConstantTable>
}
